/*
 * 
Student 클래스 만들고
이름
나이
를 맴버로 가지게 하세요.

PrintInfo()메서드(=함수, 맴버함수)를 만들어
아래처럼 출력하세요

이름 : 홍길동
나이 : 20
*/

class Character
    {
    public string Name;
    public int Age;
    public void PrintInfo()
    {
        System.Console.WriteLine("이름 : " + Name);
        System.Console.WriteLine("나이 : " + Age);
    }
}
class Program
{
    static void Main(string[] args)
    {
        Character character = new Character();
        character.Name = "홍길동";
        character.Age = 20;
        character.PrintInfo();
    }
}

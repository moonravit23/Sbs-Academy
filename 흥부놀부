class Student
{
    public string name;
    public int age;

    public Student(string newName, int newAge)
    {
        Console.WriteLine("Student 클래스 생성자 실행");
        name = newName;
        age = newAge;
    }

    ~Student()
    {
        Console.WriteLine("Student 클래스 소멸자 실행");
    }

    public void PrintInfo()
    {
        Console.WriteLine($"이름: {name}, 나이: {age}");
    }
}

class Program
{
    public static void Main()
    {
        Student student1 = new Student("놀부", 35);
        student1.PrintInfo();
        student1 = null;

        Student student2 = new Student("흥부", 30);
        student2.PrintInfo();
        student2 = null;

        GC.Collect();
        GC.WaitForPendingFinalizers();
    }
}
